CREATE TABLE digest (id INT(11) AUTO_INCREMENT PRIMARY KEY,
					 doi VARCHAR(100),
					 title VARCHAR(255),
					 auths VARCHAR(255),
					 journal VARCHAR(255),
					 p_year INT(4),
					 should_read TEXT,
					 research_gap TEXT,
					 objective TEXT,
					 finding TEXT,
					 impact TEXT,
					 figure LONGBLOB,
					 figure_caption VARCHAR(255),
           creator VARCHAR(100),
					 create_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP );
